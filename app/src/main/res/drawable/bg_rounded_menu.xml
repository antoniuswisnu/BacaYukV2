<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="rectangle">
        <solid android:color="#FFC0E9EE"/>
        <corners android:radius="16dp"/>
        <stroke android:width="4dp" android:color="#FFC0E9EE"/>
    </shape>
</item>

<item android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp">
    <shape android:shape="rectangle">
        <solid android:color="#FFEDFDFF "/>
        <corners android:radius="12dp"/>
        <stroke android:width="4dp" android:color="#FF006774"/>
    </shape>
</item>
</layer-list>
